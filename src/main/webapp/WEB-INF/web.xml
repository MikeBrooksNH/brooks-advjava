<web-app version="3.0"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>RequestInfoExample</servlet-name>
        <servlet-class>edu.mbrooks.advanced.servlet.RequestInfo</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>RequestWebQuoteExample</servlet-name>
        <servlet-class>edu.mbrooks.advanced.servlet.RequestWebQuoteInfo</servlet-class>
    </servlet>


    <servlet>
        <servlet-name>ServletForwardingExample</servlet-name>
        <servlet-class>edu.mbrooks.advanced.servlet.MoodSurvey</servlet-class>
    </servlet>


    <servlet>
        <servlet-name>RequestQuoteExample</servlet-name>
        <servlet-class>edu.mbrooks.advanced.servlet.RequestQuoteInfo</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>ServletForwardingExample</servlet-name>
        <url-pattern>/servlets/ServletForwardingExample/*</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>RequestInfoExample</servlet-name>
        <url-pattern>/servlets/RequestInfoExample/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RequestQuoteExample</servlet-name>
        <url-pattern>/servlets/RequestQuoteInfoExample/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RequestWebQuoteExample</servlet-name>
        <url-pattern>/servlets/RequestWebQuoteInfoExample/*</url-pattern>
    </servlet-mapping>




</web-app>
